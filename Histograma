numeros_positivos = 0
numeros_negativos = 0

while True:
    valor = int(input("Ingrese un nÃºmero entero (Termine con 0): "))

    if valor == 0:
        break
    elif valor > 0:
        numeros_positivos += 1
    else:
        numeros_negativos += 1

print("Valores positivos:")
for _ in range(numeros_positivos):
    print("* ", end="")

print()

print("Valores negativos:")
for _ in range(numeros_negativos):
    print("* ", end="")
